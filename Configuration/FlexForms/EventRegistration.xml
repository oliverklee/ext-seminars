<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <sheets>
        <general>
            <ROOT>
                <TCEforms>
                    <!--
                        The sheet title will not be visible in the BE. Still, the XML element needs to be present to
                        avoid a crash in the BE.
                    -->
                    <sheetTitle/>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.pageForMissingEvent>
                        <TCEforms>
                            <label>
                                LLL:EXT:seminars/Resources/Private/Language/locallang.xlf:plugin.eventRegistration.settings.pageForMissingEvent
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                                <hideSuggest>1</hideSuggest>
                            </config>
                        </TCEforms>
                    </settings.pageForMissingEvent>
                    <settings.loginPage>
                        <TCEforms>
                            <label>
                                LLL:EXT:seminars/Resources/Private/Language/locallang.xlf:plugin.eventRegistration.settings.loginPage
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                                <hideSuggest>1</hideSuggest>
                            </config>
                        </TCEforms>
                    </settings.loginPage>
                    <settings.registrationRecordsStorageFolder>
                        <TCEforms>
                            <label>
                                LLL:EXT:seminars/Resources/Private/Language/locallang.xlf:plugin.eventRegistration.settings.registrationRecordsStorageFolder
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                                <hideSuggest>1</hideSuggest>
                            </config>
                        </TCEforms>
                    </settings.registrationRecordsStorageFolder>
                    <settings.maximumBookableSeats>
                        <TCEforms>
                            <label>
                                LLL:EXT:seminars/Resources/Private/Language/locallang.xlf:plugin.eventRegistration.settings.maximumBookableSeats
                            </label>
                            <config>
                                <type>input</type>
                                <range>
                                    <lower>1</lower>
                                    <upper>999</upper>
                                </range>
                                <size>3</size>
                                <max>3</max>
                                <eval>int,nospace,trim</eval>
                                <default>10</default>
                            </config>
                        </TCEforms>
                    </settings.maximumBookableSeats>
                </el>
            </ROOT>
        </general>
    </sheets>
</T3DataStructure>
